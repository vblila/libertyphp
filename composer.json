{
    "name": "libertyphp/libertyphp",
    "description": "The Liberty PHP framework. Fast and extensible micro framework for PHP.",
    "version": "1.2",
    "type": "library",
    "keywords": [
        "php",
        "framework",
        "micro framework",
        "mvc framework",
        "fast framework"
    ],
    "homepage": "https://github.com/vblila/libertyphp",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Lila",
            "email": "v.b.lila@yandex.ru",
            "homepage": "https://github.com/vblila",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/log": "^1.1.4",
        "psr/container": "^2.0.2",
        "psr/http-message": "^1.0.1",
        "psr/http-server-middleware": "^1.0.1",
        "guzzlehttp/psr7": "^2.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.16"
    },
    "autoload": {
        "psr-4": {
            "Libertyphp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Libertyphp\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
